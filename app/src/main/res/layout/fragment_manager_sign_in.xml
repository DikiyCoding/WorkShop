<?xml version="1.0" encoding="utf-8"?>
<com.nex3z.flowlayout.FlowLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    app:flChildSpacing="auto"
    app:flChildSpacingForLastRow="align"
    app:flFlow="true"
    app:flMaxRows="5"
    app:flRowSpacing="8dp"
    app:flRtl="false">

    <Button
        android:id="@+id/btn_sign_in_facebook_login"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="1" />

    <Button
        android:id="@+id/btn_sign_in_facebook_custom"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="2" />

    <Button
        android:id="@+id/btn_sign_in_facebook_swith_user"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="3" />

    <Button
        android:id="@+id/btn_sign_in_vkontakte"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="4" />

    <Button
        android:id="@+id/btn_sign_in_twitter"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="5" />

    <Button
        android:id="@+id/btn_sign_in_google"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="6" />

    <Button
        android:id="@+id/btn_dialog_fifth_example"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="7" />

    <Button
        android:id="@+id/btn_dialog_sixth_example"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="8" />

</com.nex3z.flowlayout.FlowLayout>